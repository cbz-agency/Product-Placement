# Server-side API keys (never commit real values)

# Nano Banana
# If Nano Banana is accessed via Google AI Studio, set this key
GOOGLE_AI_STUDIO_API_KEY=
# If Nano Banana provides its own key, set it here
NANOBANANA_API_KEY=
# Optional base URL or model identifier for Nano Banana
NANOBANANA_API_BASE=
NANOBANANA_MODEL_ID=

# Product placement provider (choose and use one for MVP)
# OpenAI Images API
OPENAI_API_KEY=
# Replicate (Stable Diffusion / ControlNet / Inpainting)
REPLICATE_API_TOKEN=

# Supabase (storage + optional DB)
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Server-only key if we perform privileged actions server-side
SUPABASE_SERVICE_ROLE_KEY=
# Storage bucket name for uploads/outputs
SUPABASE_BUCKET=mockups

# App config
# Public site URL (used in links/callbacks); optional for local dev
NEXT_PUBLIC_APP_URL=
# If accessing Supabase from the client, prefer public vars:
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
